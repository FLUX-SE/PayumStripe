{
    "name": "flux-se/payum-stripe",
    "description": "Payum Stripe gateways",
    "type": "library",
    "keywords": [
        "stripe",
        "payum",
        "payment",
        "authorize"
    ],
    "require": {
        "payum/core": "^1.6",
        "stripe/stripe-php": "^7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8|^9",
        "symplify/easy-coding-standard": "^8|^9",
        "php-http/guzzle6-adapter": "^2.0",
        "phpstan/phpstan": "^1",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": { "FluxSE\\PayumStripe\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\FluxSE\\PayumStripe\\": "tests/" }
    },
    "license": "MIT",
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "authors": [
        {
            "name": "Francis HILAIRE",
            "email": "Prometee@users.noreply.github.com"
        }
    ]
}
